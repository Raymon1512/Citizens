$(document).ready(function(){'use strict';var countiesSelect=$('select#counties'),familyMemberRadioButtons=$('input[name="family-member"]'),membershipEligibilityQuestions=$('.membership-eligibility-questions'),eligibleMessage=$('.eligible-message'),notEligibleMessage=$('.not-eligible-message'),countyEligibility,familyEligibility;function checkEligibility(){if(countyEligibility===true||familyEligibility===true){membershipEligibilityQuestions.hide();eligibleMessage.show();}
if(countyEligibility===false&&familyEligibility===false){membershipEligibilityQuestions.hide();notEligibleMessage.show();}}
function checkCounties(){if(countiesSelect.val()===''||countiesSelect.val()===undefined){countyEligibility=undefined;}else if(countiesSelect.val()==='No'){countyEligibility=false;}else{countyEligibility=true;}}
function checkFamily(){if(familyMemberRadioButtons.filter(':checked').val()===undefined){familyEligibility=undefined;}else if(familyMemberRadioButtons.filter(':checked').val()==='No'){familyEligibility=false;}else{familyEligibility=true;}}
countiesSelect.on('change',function(){checkCounties();checkEligibility();});familyMemberRadioButtons.on('change',function(){checkFamily();checkEligibility();});});